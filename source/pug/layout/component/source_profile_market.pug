mixin entel-profile-market(filter,img_path,img_vp,extension,alt_img,text,src_img)
    a(href='javascript:void(0)' filter= filter).powerlike_profile_market__options__button
        img.icon-vp(src=img_path + img_vp + extension, srcset= img_path + img_vp + '@2x' + extension + ' 2x,' + img_path + img_vp + extension + ' 1x', alt=alt_img)
        span !{text}
        img.icon-arrow(src=img_path + src_img + extension, srcset= img_path + src_img + '@2x' + extension + ' 2x,' + img_path + src_img + extension + ' 1x', alt=alt_img)