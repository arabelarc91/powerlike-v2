section.powerlike_chooseprofile.influencer
	.powerlike_chooseprofile__content
		h5  Influencer
			span ¿Qué haces con tu celular?

		.powerlike_chooseprofile__slidecontainer
			.powerlike_chooseprofile__slidecontainer__item
				+range-slider("range-influencer-apps", "range-frecuence", "1" , "100", "50", "Más de 5 horas", "¿Con qué frecuencia ingresas a Facebook o Instagram durante el día?")

			.powerlike_chooseprofile__slidecontainer__item
				+range-slider("range-influencer-nivel", "range-apps", "1" , "100", "50", "Mucho", "¿Qué nivel de youtube lover eres?")

		.powerlike_chooseprofile__select
			p ¿Qué marca prefieres?
			
			div Seleccionar
				.select-icon
					span
					span 
			ul 
				- var brands = ["Samsung", "Huawei", "Apple"];
				for name in brands
					li(data-brand= name)= name

		.button_entel__carousel
			a(href="javascript:void(0)") continuar
			

